{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs16",
      "resources": {
        "timeOutSeconds": 300,
        "memoryLimit": "2GB"
      },
      "lint": [
        {
          "include": "**/*.js"
        }
      ],
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ]
}
